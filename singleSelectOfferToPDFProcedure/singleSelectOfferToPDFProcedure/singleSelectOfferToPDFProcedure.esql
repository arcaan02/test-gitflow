	SET rOutBody00.(XMLNSC.Attribute)name = 'accounts'; 
	
     ------  -- INICIO Lista de Productos Aceptados  [ACEPTADOS] ------------------------------------------------------------------------------------------------------------

     --lista de CUENTAS aceptados
	SET index0		=1;		
