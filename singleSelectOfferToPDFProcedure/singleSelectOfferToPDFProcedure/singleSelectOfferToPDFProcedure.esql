			SET rOutBody.loan.subproduct.mnemonic						=rSSLoanDetail.loan.subproduct.mnemonic;	
			SET rOutBody.loan.subproduct.product.isoProduct.mnemonic	=rSSLoanDetail.loan.subproduct.product.isoProduct.mnemonic;	
			
